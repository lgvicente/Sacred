<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">

	<managed-bean>
		<managed-bean-name>sessaoBean</managed-bean-name>
		<managed-bean-class>br.com.sacred.session.SessaoBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>inicialBean</managed-bean-name>
		<managed-bean-class>br.com.sacred.view.InicialBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>	
	
	<managed-bean>
		<managed-bean-name>inicialViewHelper</managed-bean-name>
		<managed-bean-class>br.com.sacred.view.helper.InicialViewHelper</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>	
	
   <navigation-rule>
        <from-view-id>/SacredElement.jsp</from-view-id>
        <navigation-case>
            <from-outcome>SacredElement</from-outcome>
            <to-view-id>/SacredElement.jsp</to-view-id>
   		</navigation-case>
   </navigation-rule>

</faces-config>
